{
    "name": "vanilo/channel",
    "description": "Vanilo Channel Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["channel", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/vanilophp/channel"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.2",
        "konekt/concord": "~1.5@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2 - 7.5|~8.0",
        "orchestra/testbench": "~3.5|~4.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Channel\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Channel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
