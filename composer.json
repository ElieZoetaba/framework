{
    "name": "vanilo/checkout",
    "description": "Vanilo Checkout Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["checkout", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "source": "https://github.com/vanilophp/checkout"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "konekt/concord": "~1.1",
        "vanilo/contracts": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2 - 7.5",
        "orchestra/testbench": "^3.4.2",
        "vanilo/support": "1.0.*"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Checkout\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Checkout\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
