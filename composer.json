{
    "name": "vanilo/properties",
    "description": "Vanilo Properties (Attributes) Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["product", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/vanilophp/properties"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "konekt/concord": "~1.1",
        "cviebrock/eloquent-sluggable": "~4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2 - 7.5",
        "orchestra/testbench": "^3.4.2"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Properties\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Properties\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}
