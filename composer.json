{
    "name": "vanilo/support",
    "description": "Common Vanilo functions, traits, helpers",
    "type": "library",
    "license": "MIT",
    "keywords": ["ecommerce", "laravel", "vanilo", "artkonekt"],
    "support": {
        "source": "https://github.com/vanilophp/support"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~6.0|~7.0",
        "php-decimal/php-decimal": "~1.1",
        "vanilo/contracts": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Support\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "spatie/laravel-medialibrary": "Allows associating images (or other files) with Eloquent models"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
