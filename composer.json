{
    "name": "vanilo/order",
    "description": "Vanilo Order Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["order", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/artkonekt/order"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.2",
        "konekt/concord": "~1.5@dev",
        "konekt/user": "~1.3",
        "konekt/address": "~1.1",
        "konekt/laravel-migration-compatibility": "~1.1",
        "vanilo/contracts": "~2.0@dev",
        "vanilo/support": "~2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2 - 7.5|~8.0",
        "orchestra/testbench": "~3.5|~4.0",
        "orchestra/database": "~3.5|~4.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Order\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Order\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
